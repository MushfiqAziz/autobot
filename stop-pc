export USER=root
export HOME=/home

vncserver -kill :0
rm -rf /tmp/.X0-lock
rm -rf /tmp/.X11-unix/X0
rm -rf /root/.vnc/localhost:0.pid
rm -rf /root/.vnc/localhost:0.log
vncserver -kill :1
rm -rf /tmp/.X1-lock
rm -rf /tmp/.X11-unix/X1
rm -rf /root/.vnc/localhost:1.pid
rm -rf /root/.vnc/localhost:1.log
vncserver -kill :2
rm -rf /tmp/.X2-lock
rm -rf /tmp/.X11-unix/X2
rm -rf /root/.vnc/localhost:2.pid
rm -rf /root/.vnc/localhost:2.log
